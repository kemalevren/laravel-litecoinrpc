{
    "name": "majestic/laravel-litecoinrpc",
    "type": "library",
    "description": "Litecoin JSON-RPC Service Provider for Laravel",
    "keywords": ["laravel","litecoin","api","jsonrpc","guzzle"],
    "homepage": "https://github.com/majestic84/laravel-litecoinrpc",
    "license": "MIT",
    "authors": [
        {
            "name": "majestic84",
            "email": "southsideprod@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*",
        "majestic/php-litecoinrpc": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.3.*",
        "orchestra/testbench": "3.x",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4" : {
            "Majestic\\Litecoin\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Majestic\\Litecoin\\Providers\\ServiceProvider"
            ]
        }
    }
}
